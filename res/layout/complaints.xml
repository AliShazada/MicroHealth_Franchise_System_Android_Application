<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#add8e6" >

    <ScrollView
        android:id="@+id/Scrollview__complaints"
        android:layout_width="fill_parent"
        android:layout_height="470dp"
        android:layout_below="@+id/complaints_text" >

        <RelativeLayout
            android:id="@+id/relative_layout_complaints"
            android:layout_width="wrap_content"
            android:layout_height="470dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="20dp" >

            <CheckBox
                android:id="@+id/headache_complaints"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:text="Headache"
                android:textColor="#000000"
                 android:textSize="20dp"
                android:textStyle="bold" />

            <CheckBox
                android:id="@+id/high_BP_complaints"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/headache_complaints"
                android:layout_marginLeft="10dp"
                android:text="High Blood Pressure"
                android:textColor="#000000"
                 android:textSize="20dp"
                android:textStyle="bold" />

            <CheckBox
                android:id="@+id/high_Blood_Sugar_levels_complaints"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/high_BP_complaints"
                android:layout_marginLeft="10dp"
                android:text="High Blood Sugar levels"
                android:textColor="#000000"
