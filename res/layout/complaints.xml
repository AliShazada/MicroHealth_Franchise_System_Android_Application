            <CheckBox
                android:id="@+id/Serizures_complaints"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Reduced_Apptitite_complaints"
                android:layout_marginLeft="10dp"
                android:text="Serizures"
                android:textColor="#000000"
                 android:textSize="20dp"
                android:textStyle="bold" />

            <CheckBox
                android:id="@+id/Shortness_of_breath_complaints"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Serizures_complaints"
                android:layout_marginLeft="10dp"
