            <CheckBox
                android:id="@+id/Contractions_complaints"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Blurred_Vision_complaints"
                android:layout_marginLeft="10dp"
