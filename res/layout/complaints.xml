                android:textStyle="bold" />

            <CheckBox
                android:id="@+id/Nausea_complaints"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Look_pale_complaints"
                android:layout_marginLeft="10dp"
