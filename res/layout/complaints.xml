                android:textColor="#000000"
                 android:textSize="20dp"
                android:textStyle="bold" />

            <CheckBox
                android:id="@+id/Fetal_movments_felt_in_Different_complaints"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Feel_tired_complaints"
                android:layout_marginLeft="10dp"
                android:text="Fetal movments felt in Different Parts"
                android:textColor="#000000"
                 android:textSize="20dp"
                android:textStyle="bold" />

            <CheckBox
                android:id="@+id/Fluid_or_Tissue_passing_through_vagina_complaints"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Fetal_movments_felt_in_Different_complaints"
                android:layout_marginLeft="10dp"
                android:text="Fluid or Tissue passing through vagina"
                android:textColor="#000000"
                 android:textSize="20dp"
                android:textStyle="bold" />

            <!-- <CheckBox -->
