                android:textColor="#000000"
                 android:textSize="20dp"
                android:textStyle="bold" />

            <CheckBox
                android:id="@+id/Baby_is_smaller_than_normal_complaints"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Abdominal_pain_complaints"
                android:layout_marginLeft="10dp"
                android:text="Baby is smaller than normal"
                android:textColor="#000000"
                 android:textSize="20dp"
                android:textStyle="bold" />

            <CheckBox
                android:id="@+id/Baby_is_moving_less_than_normal_complaints"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
