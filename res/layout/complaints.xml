                 android:textSize="20dp"
                android:textStyle="bold" />

            <CheckBox
                android:id="@+id/Utirine_tenderness_complaints"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Too_much_Protein_in_urine_complaints"
                android:layout_marginLeft="10dp"
                android:text="Utirine Tenderness"
                android:textColor="#000000"
                 android:textSize="20dp"
                android:textStyle="bold" />

            <CheckBox
                android:id="@+id/Uterus_is_larger_than_expected_complaints"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Utirine_tenderness_complaints"
                android:layout_marginLeft="10dp"
                android:text="Uterus is larger than expected"
                android:textColor="#000000"
                 android:textSize="20dp"
                android:textStyle="bold" />

            <CheckBox
                android:id="@+id/Vaginal_Bleeding_complaints"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Uterus_is_larger_than_expected_complaints"
                android:layout_marginLeft="10dp"
