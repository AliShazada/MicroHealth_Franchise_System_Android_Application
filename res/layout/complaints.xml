            <CheckBox
                android:id="@+id/Pelvic_pressure_complaints"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Fluid_or_Tissue_passing_through_vagina_complaints"
                android:layout_marginLeft="10dp"
                android:text="Pelvic Pressure"
                android:textColor="#000000"
                 android:textSize="20dp"
                android:textStyle="bold" />

            <CheckBox
                android:id="@+id/Reduced_Apptitite_complaints"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Pelvic_pressure_complaints"
                android:layout_marginLeft="10dp"
