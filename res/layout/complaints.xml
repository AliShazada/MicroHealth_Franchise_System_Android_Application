            <CheckBox
                android:id="@+id/Fainting_complaints"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Extreme_thirst_complaints"
                android:layout_marginLeft="10dp"
                android:text="Fainting"
                android:textColor="#000000"
                 android:textSize="20dp"
                android:textStyle="bold" />

            <CheckBox
                android:id="@+id/Fatigue_complaints"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Fainting_complaints"
                android:layout_marginLeft="10dp"
                android:text="Fatigue"
                android:textColor="#000000"
                 android:textSize="20dp"
                android:textStyle="bold" />

            <CheckBox
                android:id="@+id/Feel_tired_complaints"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Fatigue_complaints"
                android:layout_marginLeft="10dp"
